# OpenIM Admin New Front 环境变量配置

# 应用配置
NODE_ENV=production

# 前端服务镜像配置
OPENIM_ADMIN_NEW_FRONT_1_IMAGE=openim-admin-new-front-1:latest
OPENIM_ADMIN_NEW_FRONT_2_IMAGE=openim-admin-new-front-2:latest

# 后端服务配置
# Front 1 代理到 47.239.126.22
BACKEND_SERVER_1=47.239.126.22

# Front 2 代理到 47.83.254.218
BACKEND_SERVER_2=47.83.254.218

# 管理后台服务端口
ADMIN_SERVER_PORT=10009

# 用户服务端口
USER_SERVER_PORT=10008

# IM系统服务端口
IM_SERVER_PORT=10002

# 前端服务端口
FRONT_1_PORT=11003
FRONT_2_PORT=11004

# SSL配置
SSL_ENABLED=false
SSL_CERT_PATH=/etc/nginx/ssl/cert.crt
SSL_KEY_PATH=/etc/nginx/ssl/cert.key

# 日志配置
LOG_LEVEL=info
LOG_DIR=/var/log/nginx

# 健康检查配置
HEALTH_CHECK_INTERVAL=30s
HEALTH_CHECK_TIMEOUT=3s
HEALTH_CHECK_RETRIES=3
HEALTH_CHECK_START_PERIOD=5s
